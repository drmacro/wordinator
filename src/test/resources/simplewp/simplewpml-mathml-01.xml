<?xml version="1.0" encoding="UTF-8"?>
<wp:document xmlns:wp="urn:ns:wordinator:simplewpml" xmlns="http://www.w3.org/1999/xhtml">
  <wp:body>
    <wp:p>
      <wp:run>This is a line of text followed by an equation.</wp:run>
    </wp:p>
    <wp:p>
      <mml:math xmlns:mml="http://www.w3.org/1998/Math/MathML" display="block">
        <mml:mrow>
          <mml:msub>
            <mml:mi>F</mml:mi>
            <mml:mtext>T</mml:mtext>
          </mml:msub>
          <mml:mo>=</mml:mo>
          <mml:mfrac>
            <mml:mrow>
              <mml:mn>1</mml:mn>
              <mml:mo>,</mml:mo>
              <mml:mn>5</mml:mn>
              <mml:mo>×</mml:mo>
              <mml:msub>
                <mml:mi>p</mml:mi>
                <mml:mtext>T</mml:mtext>
              </mml:msub>
              <mml:mo>×</mml:mo>
              <mml:mi>π</mml:mi>
              <mml:mo>×</mml:mo>
              <mml:msup>
                <mml:mrow>
                  <mml:mrow>
                    <mml:mo>(</mml:mo>
                    <mml:mrow>
                      <mml:msub>
                        <mml:mi>d</mml:mi>
                        <mml:mtext>n</mml:mtext>
                      </mml:msub>
                      <mml:mo>−</mml:mo>
                      <mml:msub>
                        <mml:mi>e</mml:mi>
                        <mml:mtext>m</mml:mtext>
                      </mml:msub>
                    </mml:mrow>
                    <mml:mo>)</mml:mo>
                  </mml:mrow>
                </mml:mrow>
                <mml:mn>2</mml:mn>
              </mml:msup>
            </mml:mrow>
            <mml:mrow>
              <mml:mn>20</mml:mn>
            </mml:mrow>
          </mml:mfrac>
        </mml:mrow>
      </mml:math>
    </wp:p>
  </wp:body>
</wp:document>
